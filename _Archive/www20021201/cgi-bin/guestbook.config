<-guestbook.password->        # Encrypted password for Admin Web-access.
Ch6C6x/eskGR6

<-guestbook.language->        # Name of the language file. English if none.
guestbook-xx.lang

<-guestbook.reverse_sort->    # Yes = 1, No = 0
1

<-guestbook.show_time->       # Yes = 1, No = 0
1

<-guestbook.TIME_FORMAT->     # H=Hours, M=Minutes, S=Seconds
H:M

<-guestbook.auto_br->         # Yes = 1, No = 0
1

<-guestbook.email_on_name->   # Yes = 1, No = 0. Puts mailto-link on Name-text.
0

<-guestbook.allow_double->    # Don't filter double entries. Yes=1, No=0
0

<-guestbook.pagetitle->       # Title of all pages generated
My guestbook!

<-guestbook.font_size->       # The size of the messagefont
3

##############################################################################
# Anti-spam function:
# Changes the way e-mail addresses are shown in the guestbook.
#
# 0 = E-mail addresses are shown as they are, no modifications.
# 1 = The "@" is exchanged with " at " in both the links and the mailto tag.
# 2 = As above, but this time there is no link, just the text.
##############################################################################

<-guestbook.email_antispam->
0

##############################################################################
# THE MAIL-OPTION MAY NOT WORK ON WINDOWS NT! See the support-page for news.
##############################################################################

<-guestbook.mailprogram->     # The name and path of the mailprogram.
/usr/sbin/sendmail

##############################################################################
# Options to remove unwanted HTML-tags from the entries
##############################################################################

<-guestbook.show_images->     # Allow images in the guestbook. Yes = 1, No = 0
1

<-guestbook.strip_html->      # Strips ALL html-tags.  Yes = 1, No = 0
0

<-guestbook.allow_scripts->   # Don't remove SCRIPTS from entries. Yes=1, No=0
0

<-guestbook.blink_off->       # Remove BLINK-tags. Yes=1, No=0
1

<-guestbook.meta_off->        # Remove META-tags. Yes=1, No=0 
1

<-guestbook.forms_off->       # Remove FORM-tags. Yes=1, No=0 
1

<-guestbook.applets_off->     # Remove APPLET-tags. Yes=1, No=0
1

<-guestbook.object_off->      # Remove OBJECT-tags. Yes=1, No=0
1

<-guestbook.embed_off->       # Remove EMBED-tags. Yes=1, No=0
1

<-guestbook.bgsound_off->     # Remove BGSOUND-tags. Yes=1, No=0
1

##############################################################################
# This option shows the number of hits each guestbook-page has got.
#
# NOTE:  THIS OPTION MAY NOT BE IMPLEMENTED YET!
#        See the History-part of the support-page to check newer versions.
##############################################################################

<-guestbook.show_hits->       # Yes = 1, No = 0
0

##############################################################################
# The number of entries that are shown.
# Only the x most recent entries are shown.          
# 0 = Show all entries
##############################################################################

<-guestbook.entries_shown->
0

##############################################################################
# Email notification.
# If "do_sendmail" is set to 1, the entries are emailed to "mailaddress".
##############################################################################

<-guestbook.do_sendmail->     # Yes = 1, No = 0
1

<-guestbook.mailaddress->
anso@mail.ru

##############################################################################
# If "mailto_guest" is set to 1, the content in "mailto_guest_message" is
# emailed to the guest if the guest entered his or her email-address.
##############################################################################

<-guestbook.mailto_guest->               # Yes = 1, No = 0
0
    
<-guestbook.mailto_guest_from->          # Your/company name
Guestbook owner

<-guestbook.mailto_guest_from_address->  # Your e-mail address
your@email.address

<-guestbook.mailto_guest_subject->       # Subject of the e-mail
Thanks!

<-guestbook-mult.mailto_guest_message->
Hi there!

Thank you for signing the guestbook!
This e-mail was automaticly sent to you by the guestbook-system.

Best regards.

